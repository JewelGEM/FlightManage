<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.cc.dao.StudentDao">

    <insert id="addStudent">
        insert into t_student values(null,#{sname},#{sex},#{age},#{c.cid});
    </insert>

    <select id="getStudentAll" resultType="com.cc.pojo.T_Student">
        SELECT * FROM t_student
    </select>


</mapper>